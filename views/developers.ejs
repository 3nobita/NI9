        utilsScript: "https://www.propvestors.in/wp-content/themes/propvestors/js/utils/js/utils.js",
      });
    }
    jQuery("input.numeric").bind({
      keydown: function (e) {
        if (e.shiftKey === true) {
          if (e.which == 9) {
            return true;
          }
          return false;
        }
        if (e.which > 57) {
          return false;
        }
        if (e.which == 32) {
          return false;
        }
        return true;
      }
    });
    jQuery("input.phone").on("keyup", function (event) {
      var element = jQuery(this);
      var india = true;
      var phone = jQuery(this).val();
      var check = element.closest('form').find('.iti__flag').hasClass('iti__in');
      if (check == true) {
        element.closest('form').find('input.otp').attr('required', true);
        element.closest('form').find('input.phone').attr('minlength', '10');
        element.closest('form').find('input.phone').attr('maxlength', '10');
        india = true;
      } else {
        india = false;
        element.closest('form').find('input.otp').attr('required', false);
        element.closest('form').find('input.phone').attr('minlength', '5');
        element.closest('form').find('input.phone').attr('maxlength', '15');
      }
      if (this.value.length == this.getAttribute('maxlength') && india == true) {
        jQuery.ajax({
          type: 'POST',
          url: 'https://www.propvestors.in/wp-content/themes/propvestors/sendotp.php',
          data: 'phone=' + phone,
          success: function () {
            element.closest('form').find('.otp-wrapper').show();
          }
        });
      } else {
        element.closest('form').find('.otp-wrapper').hide();
        element.closest('form').find('input.otp').val('');
      }
    });
    jQuery("input.otp").on("keyup", function (event) {
      var element = jQuery(this);
      if (this.value.length == this.getAttribute('maxlength')) {
        var otp = jQuery(this).val();
        jQuery.ajax({
          type: 'POST',
          url: 'https://www.propvestors.in/wp-content/themes/propvestors/verifyotp.php',
          data: 'otp=' + otp,
          success: function (data) {
            if (data == 'success') {
              element.closest('form').find('.otpHelp').html('Mobile number verified.').removeClass('text-warning').addClass('text-success');
            } else {
              element.closest('form').find('input.otp').val('');
              element.closest('form').find('.otpHelp').html('Please enter correct OTP.').removeClass('text-success').addClass('text-danger');
            }
          }
        });
      }
    });
  </script>
