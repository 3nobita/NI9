      <style>
         #maino {
              margin: 0;
              height: 100vh;
              display: flex;
              align-items: center;
              justify-content: center;
              background-color: #f0f0f0; /* Light background for the body */
          }
  
          #overlayo {
              display: none; /* Hidden by default */
              position: absolute;
              top: 0;
              left: 0;
              width: 100%;
              height: 100%;
              background-color: rgba(0, 0, 0, 0.7); /* Dark overlayo */
              z-index: 999; /* Ensure it's above other content */
          }
  
          #popup-form {
              display: none;
              transform: translate(-50%, -50%);
              background: #fff;
              box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
              padding: 20px;
              border-radius: 10px;
              z-index: 1000;
              width: 400px;
              animation: pop 0.5s ease forwards;
          }
  
          @keyframes pop {
              0% {
                  transform: scale(0);
                  opacity: 0;
              }
  
              100% {
                  transform: scale(1);
                  opacity: 1;
              }
          }
  
          .input-box {
              margin-bottom: 15px;
          }
  
          .input-box input {
              border: 1px solid #ccc;
              padding: 10px;
              border-radius: 5px;
              width: calc(100% - 22px);
          }
  
          #close-popup {
              background: none;
              border: none;
              cursor: pointer;
              position: absolute;
              top: 10px;
              right: 10px;
              font-size: 20px;
              color: #888;
          }
  
          h2 {
              color: #333;
              text-align: center;
          }
  
